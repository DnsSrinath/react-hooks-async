{
  "name": "react-hooks-async",
  "description": "An abortable async function library with React Hooks",
  "version": "0.5.1",
  "author": "Daishi Kato",
  "repository": {
    "type": "git",
    "url": "https://github.com/dai-shi/react-hooks-async.git"
  },
  "main": "./dist/index.js",
  "module": "./src/index.js",
  "types": "./src/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "compile": "babel src -d dist",
    "test": "npm run eslint && npm run tslint && npm run tsc-test && npm run jest",
    "eslint": "eslint --ignore-pattern dist .",
    "jest": "jest",
    "tslint": "tslint --project .",
    "tsc-test": "tsc --project . --noEmit",
    "examples:minimal": "DIR=01_minimal EXT=js webpack-dev-server",
    "examples:typescript": "DIR=02_typescript webpack-dev-server",
    "examples:startbutton": "DIR=03_startbutton webpack-dev-server",
    "examples:typeahead": "DIR=04_typeahead webpack-dev-server",
    "examples:axios": "DIR=05_axios webpack-dev-server",
    "examples:progress": "DIR=06_progress webpack-dev-server",
    "examples:race": "DIR=07_race webpack-dev-server"
  },
  "keywords": [
    "react",
    "hooks",
    "fetch"
  ],
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@types/node-fetch": "^2.1.4",
    "@types/react": "^16.7.13",
    "@types/react-dom": "^16.0.11",
    "axios": "^0.18.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "ts-loader": "^5.3.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.2.2",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "peerDependencies": {
    "react": ">=16.7.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "browser": true
    },
    "globals": {
      "AbortController": false
    },
    "extends": [
      "airbnb"
    ],
    "rules": {
      "import/prefer-default-export": 0,
      "react/jsx-filename-extension": [
        2,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "import/no-extraneous-dependencies": [
        2,
        {
          "devDependencies": true
        }
      ],
      "import/no-unresolved": [
        2,
        {
          "ignore": [
            "react-hooks-async"
          ]
        }
      ],
      "react/prop-types": 0
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage"
        }
      ],
      "@babel/preset-react"
    ]
  }
}
